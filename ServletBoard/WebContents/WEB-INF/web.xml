<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ServletBoard</display-name>
  <welcome-file-list>
    <welcome-file>/WEB-INF/view/board/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>boardView</servlet-name>
  	<servlet-class>com.board.controller.BoardView</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>boardView</servlet-name>
  	<url-pattern>/board/view</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>boardInsert</servlet-name>
  	<servlet-class>com.board.controller.BoardInsert</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>boardInsert</servlet-name>
  	<url-pattern>/board/insert</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>boardUpdate</servlet-name>
  	<servlet-class>com.board.controller.BoardUpdate</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>boardUpdate</servlet-name>
  	<url-pattern>/board/update</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>boardDelete</servlet-name>
  	<servlet-class>com.board.controller.BoardDelete</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>boardDelete</servlet-name>
  	<url-pattern>/board/delete</url-pattern>
  </servlet-mapping>
</web-app>